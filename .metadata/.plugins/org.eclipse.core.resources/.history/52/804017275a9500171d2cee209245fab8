import java.util.*;

public class Song {
	public static void main(String[] args){
		Collection coll = new ArrayList();
		coll.add("1");
		coll.add("2");
		coll.add("3");
		coll.add("4");
		coll.remove("3");
		
		Iterator irr = coll.iterator();
		
		while(irr.hasNext()){
			System.out.println(irr.next());
		}
		
		for(Object o: coll){
			System.out.println(o);
		}
		
	}
}
